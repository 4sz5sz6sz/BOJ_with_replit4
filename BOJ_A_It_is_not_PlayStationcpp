#include<bits/stdc++.h>
using namespace std;

int main(){
	ios::sync_with_stdio(0);
	cin.tie(0); cout.tie(0);

	int N;
	string s;
	cin >> N;
	cin >> s;

	string ans;
	ans.push_back(s[0]);
	if(s.size()>=2)
		ans.push_back(s[1]);

	for(int i=2;i<N;i++){
		if(s.size()<=2) break;
		//ans.push_back(s[i]);
		if(ans.size()>=2 && ans[ans.size()-2] == 'P' && ans[ans.size()-1] == 'S' && (s[i] == '4' || s[i] == '5')){
			continue;
			//ans.pop_back();
		}
		ans.push_back(s[i]);
	}
	cout << ans << "\n";
	
	return 0;
}

